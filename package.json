{
  "name": "@ghom/bot.ts",
  "version": "v8.0.0-Capi",
  "author": "ghom",
  "description": "A Discord bot in TypeScript made with [bot.ts](https://ghom.gitbook.io/bot-ts/)",
  "main": "dist/index.js",
  "type": "module",
  "style": "options",
  "imports": {
    "#app": "./dist/app.js",
    "#env": "./dist/app/env.js",
    "#config": "./dist/config.js",
    "#client": "./dist/app/client.js",
    "#logger": "./dist/app/logger.js",
    "#database": "./dist/app/database.js",
    "#src/*": "./dist/*",
    "#tables/*": "./dist/tables/*"
  },
  "scripts": {
    "bot": "npx @ghom/bot.ts-cli",
    "build": "gulp build",
    "watch": "gulp watch",
    "start": "npm run build && node dist/index",
    "start.test": "npm run build && node tests/index.test.js",
    "format": "prettier --write src tests",
    "lint": "eslint src/**/*.ts --fix",
    "update": "gulp update",
    "readme": "gulp readme",
    "test": "npm run lint && tsc --noEmit && npm run start.test"
  },
  "prettier": {
    "semi": false,
    "endOfLine": "crlf"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bot-ts/framework.git"
  },
  "bugs": {
    "url": "https://github.com/bot-ts/framework/issues"
  },
  "homepage": "https://github.com/bot-ts/framework#readme",
  "dependencies": {
    "@discordjs/rest": "^0.5.0",
    "@ghom/handler": "^3.1.0",
    "@ghom/logger": "^2.0.2",
    "@ghom/orm": "^1.7.2",
    "boxen": "^7.0.0",
    "dayjs": "^1.11.3",
    "dirname-filename-esm": "^1.1.1",
    "discord.js": "^14.15.2",
    "dotenv": "^16.0.1",
    "figlet": "^1.5.2",
    "ghom-eval": "^1.1.3",
    "ghom-prettify": "^3.0.0",
    "knex": "^3.0.1",
    "prettier": "^3.2.5",
    "regex-parser": "^2.2.11",
    "simple-git": "^3.24.0",
    "sqlite3": "latest",
    "tims": "^2.1.0",
    "types-package-json": "^2.0.39",
    "yargs-parser": "^21.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@esbuild/linux-x64": "^0.20.1",
    "@ghom/bot.ts-cli": "^8.1.3",
    "@types/figlet": "^1.5.4",
    "@types/node": "^22.0.2",
    "@types/prettier": "^2.6.3",
    "@types/ws": "^8.5.3",
    "@types/yargs-parser": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "discord-api-types": "^0.36.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "esm": "^3.2.25",
    "gulp": "^5.0.0",
    "typescript": "^5.4.0-beta"
  },
  "engines": {
    "node": ">=22.x.x",
    "npm": ">=10.x.x",
    "git": ">=2.x.x"
  },
  "optionalDependencies": {
    "del": "^6.1.1",
    "fancy-log": "^2.0.0",
    "git-commit-info": "^2.0.2",
    "gulp-esbuild": "^0.12.1",
    "gulp-filter": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "plugin-error": "^2.0.1",
    "through2": "^4.0.2",
    "vinyl-paths": "^5.0.0"
  }
}

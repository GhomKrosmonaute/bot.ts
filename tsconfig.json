{
  "compilerOptions": {
    "moduleResolution": "Node",
    "rootDir": "src",
    "module": "ESNext",
    "target": "ES2020",
    "outDir": "dist",
    "lib": ["ES2020"],
    "strict": true,
    "sourceMap": true,
    "declaration": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
  //"exclude": ["node_modules", "dist", "node_modules/discord.js/typings/index.d.ts"]
}
